class Solution {
public:
    void solve(vector<int>& nums,vector<int>& ans,vector<vector<int>>& res,vector<bool>& seen){
        if(ans.size() == nums.size()){
            res.push_back(ans);
            return;
        }
        for(int i=0;i<nums.size();++i){
            if(!seen[i]){
                seen[i] = true;
                ans.push_back(nums[i]);
                solve(nums,ans,res,seen);
                ans.pop_back();
                seen[i] = false;
            }
        }
    }
    vector<vector<int>> permute(vector<int>& nums) {
        vector<vector<int>> res;
        vector<int> ans;
        vector<bool> seen(nums.size(),false);
        solve(nums,ans,res,seen);
        return res;
    }
};
