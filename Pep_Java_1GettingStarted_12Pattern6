#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    long long int a=0,b=1,c;
    for(int i=0;i<n;i++){
        for(int j=0;j<=i;j++){
            cout<<a<<" ";
            c=a+b;
            a=b;
            b=c;
        }
        cout<<endl;
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
