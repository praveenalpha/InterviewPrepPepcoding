#include <bits/stdc++.h>
using namespace std;
void solve(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        vector<int> arr(n);
        int count=0,a;
        for(int i=0;i<n;++i){
            cin>>arr[i];
            if(arr[i] == 0)
                ++count;
        }
        a=count;
        while(count--)
            cout<<0<<" ";
        for(int i=0;i<n-a;++i)
            cout<<1<<" ";
        cout<<'\n';
    }
}
int main() {
    solve();
	//code
	return 0;
}
